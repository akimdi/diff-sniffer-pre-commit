{
  "name": "morozov/diff-sniffer-pre-commit",
  "description": "Git pre-commit hook for Diff Sniffer",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "require": {
    "morozov/diff-sniffer-core" : "^3.1.1",
    "php" : "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "phpstan/phpstan": "^0.9"
  },
  "autoload": {
    "psr-4": {
      "DiffSniffer\\PreCommit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DiffSniffer\\PreCommit\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.1.x-dev"
    }
  }
}
